{
  "name": "metabroker",
  "version": "0.1.1",
  "description": "Unified pub-sub API for message broker of your choice: Redis, RabbitMQ (AMQP), ActiveMQ (STOMP)",
  "main": "index.js",
  "keywords": [
    "pub-sub",
    "broker",
    "Redis",
    "RabbitMQ",
    "AMQP",
    "ActiveMQ",
    "STOMP"
  ],
  "author": {
    "name": "Vladimir Latyshev",
    "email": "latysheff@gmail.com"
  },
  "license": "MIT",
  "repository": "https://github.com/latysheff/node-metabroker.git",
  "dependencies": {
    "amqp-connection-manager": "^1.3.7",
    "amqplib": "^0.5.2",
    "redis": "^2.8.0",
    "stomp-client": "^0.9.0"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "extends": ".eslintrc",
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "max-len": [
            "error",
            {
              "code": 300
            }
          ]
        }
      }
    ]
  }
}
